{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "no-param-reassign": "off",
    "no-unused-vars": "warn"
  }
}
